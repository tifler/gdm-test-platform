PLATFORM=cygwin

ifneq ("$(windir)","")
PLATFORM=windows
endif

ifneq ("$(WinDir)","")
PLATFORM=windows
endif

ifneq ("$(WINDIR)","")
PLATFORM=windows
endif

UNAME=$(shell uname -o)

ifeq ("$(UNAME)","Cygwin")
PLATFORM=cygwin
endif

ifeq ("$(BUILD_CONFIGURATION)","")
	BUILD_CONFIGURATION=Debug_Embedded_Linux
endif

ifeq ("$(BUILD_CONFIGURATION)","Debug_Embedded_Linux")
CC= arm-none-linux-gnueabi-gcc
CXX= arm-none-linux-gnueabi-g++
AR= arm-none-linux-gnueabi-ar
CFLAGS= -g -I./jpuapi -I./include
CXXFLAGS= -g -I./jpuapi -I./include
ARFLAGS= cru
LDFLAGS= -lpthread -lm
RESULT=jpurun
INTERMEDIATE_DIR=.
DESTINATION_DIR=.
ifeq ("$(PLATFORM)","cygwin")
	RM=rm -f
	MKDIR=mkdir -p
	INTERMEDIATE_DIR_MK=$(INTERMEDIATE_DIR)
	DESTINATION_DIR_MK=$(DESTINATION_DIR)
else
	RM= rm -f
	MKDIR= mkdir -p
	INTERMEDIATE_DIR_MK=.
	DESTINATION_DIR_MK=.
endif

VPATH= $(INTERMEDIATE_DIR):jdi:jdi/linux:jpuapi:src:jdi/windows

endif

ifeq ("$(BUILD_CONFIGURATION)","Debug_NativeLinux")
