SOURCES= hpi.c jdi.c jpuapi.c jpuapifunc.c jpuhelper.c jpulog.c jpurun.c main.c mixer.c mm.c

SOURCEPATHS= jdi/linux/jdi.c jdi/mm.c jdi/nonos/jdi.c jdi/windows/hpi.c jdi/windows/jdi.c jpuapi/jpuapi.c jpuapi/jpuapifunc.c src/jpuhelper.c src/jpulog.c src/jpurun.c src/main.c src/mixer.c

HEADERS= config.h hpi.h jdi.h jmm.h jpuapi.h jpuapifunc.h jpuconfig.h jpuhelper.h jpulog.h jpurun.h jputable.h jputypes.h mixer.h mm.h regdefine.h



OBJECTNAMES=$(patsubst %.c,%.o,$(patsubst %.cpp,%.o,$(SOURCES)))
OBJECTPATHS=$(addprefix $(INTERMEDIATE_DIR)/,$(OBJECTNAMES))

$(DESTINATION_DIR)/$(RESULT): $(OBJECTNAMES)
ifeq ("$(wildcard $(DESTINATION_DIR) )","")
	-$(MKDIR) $(DESTINATION_DIR_MK)
endif
	$(CXX) $(SYSROOTFLAGS) $(OBJECTPATHS) -o $@ $(LDFLAGS)

$(INTERMEDIATE_DIR)/jpurun.d: $(SOURCES) $(HEADERS)
ifeq ("$(wildcard $(DESTINATION_DIR) )","")
	-$(MKDIR) $(DESTINATION_DIR_MK)
endif
ifeq ("$(wildcard $(INTERMEDIATE_DIR) )","")
	-$(MKDIR) $(INTERMEDIATE_DIR_MK)
endif
	$(CXX) $(CXXFLAGS) -MM $(SOURCEPATHS) > $@

-include $(INTERMEDIATE_DIR)/jpurun.d

clean:
	$(RM) $(DESTINATION_DIR)/$(RESULT)
	$(RM) $(INTERMEDIATE_DIR)/jpurun.d
	$(RM) $(OBJECTPATHS)

%.o: %.cpp
	$(CXX) $(CXXFLAGS) -c -o $(INTERMEDIATE_DIR)/$@ $<

%.o: %.c
	$(CC) $(CFLAGS) -c -o $(INTERMEDIATE_DIR)/$@ $<
